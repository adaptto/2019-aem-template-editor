<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <rep:policy/>
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <root-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Root Container"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/libs/wcm/foundation/components/responsivegrid,/apps/tpleditdemo/core/components/content/header]">
                        <jcr:content jcr:primaryType="nt:unstructured"/>
                    </root-container>
                    <content-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Content Container"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/apps/tpleditdemo/core/components/content/portfolio,/apps/tpleditdemo/core/components/content/services]">
                        <jcr:content jcr:primaryType="nt:unstructured"/>
                        <cq:authoring jcr:primaryType="nt:unstructured">
                            <assetToComponentMapping jcr:primaryType="nt:unstructured"/>
                        </cq:authoring>
                    </content-container>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
    <tpleditdemo jcr:primaryType="nt:unstructured">
        <core jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <content jcr:primaryType="nt:unstructured">
                    <services jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Default"
                            sling:resourceType="wcm/core/components/policy/policy"
                            components="[/apps/tpleditdemo/core/components/content/servicesItem,/apps/tpleditdemo/core/components/content/servicesLinkList]">
                            <jcr:content jcr:primaryType="nt:unstructured"/>
                            <cq:styleGroups jcr:primaryType="nt:unstructured">
                                <item0
                                    cq:styleGroupLabel="Background"
                                    jcr:primaryType="nt:unstructured">
                                    <cq:styles jcr:primaryType="nt:unstructured">
                                        <item0
                                            cq:styleClasses="bg-light"
                                            cq:styleId="1563879048542"
                                            cq:styleLabel="Light"
                                            jcr:primaryType="nt:unstructured"/>
                                        <item1
                                            cq:styleClasses="bg-dark"
                                            cq:styleId="1563879049591"
                                            cq:styleLabel="Dark"
                                            jcr:primaryType="nt:unstructured"/>
                                    </cq:styles>
                                </item0>
                            </cq:styleGroups>
                        </default>
                    </services>
                    <portfolio jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Default"
                            sling:resourceType="wcm/core/components/policy/policy"
                            components="/apps/tpleditdemo/core/components/content/portfolioItem">
                            <jcr:content jcr:primaryType="nt:unstructured"/>
                            <cq:styleGroups jcr:primaryType="nt:unstructured">
                                <item0
                                    cq:styleGroupLabel="Background"
                                    jcr:primaryType="nt:unstructured">
                                    <cq:styles jcr:primaryType="nt:unstructured">
                                        <item0
                                            cq:styleClasses="bg-light"
                                            cq:styleId="1563879173260"
                                            cq:styleLabel="Light"
                                            jcr:primaryType="nt:unstructured"/>
                                        <item1
                                            cq:styleClasses="bg-dark"
                                            cq:styleId="1563879174202"
                                            cq:styleLabel="Dark"
                                            jcr:primaryType="nt:unstructured"/>
                                    </cq:styles>
                                </item0>
                            </cq:styleGroups>
                        </default>
                    </portfolio>
                    <portfolioItem jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Default"
                            sling:resourceType="wcm/core/components/policy/policy"
                            actionsDisabled="true"
                            imageLinkHidden="true"
                            titleLinkHidden="true"
                            titleType="h4">
                            <jcr:content jcr:primaryType="nt:unstructured"/>
                        </default>
                    </portfolioItem>
                    <header jcr:primaryType="nt:unstructured"/>
                </content>
            </components>
        </core>
    </tpleditdemo>
</jcr:root>
